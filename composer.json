  "require": {
    "php": ">=5.6",
    "ext-pcntl": "*",
    "symfony/symfony": "^2.8|^3.0|^4.0",
    "sensio/distribution-bundle": "^4.0.0",
    "incenteev/composer-parameter-handler": "^2.0.0"
  },
  "require-dev": {
    "symfony/monolog-bundle": "^2.4.0",
    "phpunit/php-code-coverage": "~4.0",
