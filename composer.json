{
  "name": "smartbox/camel-config-bundle",
  "description": "Smartbox Camel Config Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Smartbox\\Integration\\CamelConfigBundle\\": ""
    }
  },
  "require": {
    "php": ">=5.5",
    "ext-pcntl": "*",
    "ext-mongodb": "~1.0",
    "centraldesktop/stomp": "^2.0",
    "symfony/symfony": "2.7.*",
    "doctrine/annotations": "1.2.*",
    "symfony/assetic-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "sensio/distribution-bundle": "~4.0",
    "sensio/framework-extra-bundle": "^3.0",
    "incenteev/composer-parameter-handler": "~2.0",
    "jms/serializer-bundle": "^0.13.0",
    "smartbox/core-bundle": "dev-master",
    "smartbox/framework-bundle": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.7.*",
    "sensio/generator-bundle": "~2.3"
  },
  "config": {
    "bin-dir": "bin"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "git@gitlab.production.smartbox.com:smartesb/BeSimpleSoap.git"
    },
    {
      "type": "vcs",
      "url":  "git@gitlab.production.smartbox.com:smartesb/core-bundle.git"
    },
    {
      "type": "vcs",
      "url":  "git@gitlab.production.smartbox.com:smartesb/framework.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable" : true
}
